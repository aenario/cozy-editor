#CNE_autocomplete
    display:block
    position: absolute
    _moz_resizing:false
    font-size:14px
    background-color:whiteSmoke
    border-radius:5px
    border-color:#CDCDD6
    border-style:solid
    border-width:1px
    padding:5px
    list-style-type:none

.SUGG_line
    cursor:pointer
    display : block

.SUGG_selected
    background-color:#F5F8B3

.SUGG_line > .tag
    color: #2571E2
    font-weight:bold

.SUGG_line > .tag:before
    content:'@'
    color:#2571E2

.SUGG_line > .contact:before
    content:'@'
    color:#979797

.SUGG_line > .strong
    font-weight:bold

.SUGG_line > .mention
    font-weight: lighter
    font-size: 11px
    float: right
    margin-left: 10px
    color:#808080

.SUGG_line:hover
    background-color : rgb(103, 210, 214)
    





